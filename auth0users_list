local L = game:GetService("Lighting")

-- Function to generate random folder names
local function R(n)
    local c = "abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789"
    local s = ""
    for i = 1, n do
        s = s .. c:sub(math.random(1, #c), math.random(1, #c))
    end
    return s
end

-- Create AccessFolder
local F = Instance.new("Folder")
F.Name = "AccessFolder"
F.Parent = L

-- Create nested folders
local P = F
for i = 1, 50 do
    local C = Instance.new("Folder")
    C.Name = R(8)
    C.Parent = P
    P = C
end

-- List of authorized users
local users = {
    "hitarianp05","metonmua036","hidukeyth24","elhanthef03","manielxd2009",
    "riva_io2007","idkralina00","ignnelver8","mrdosbail23","batatetop999",
    "XxMax_StealthxX2024","Prism_CIRCUIT18","Beast_Chaos12","78l2x","1hn6k",
    "FuryHawkEagl32007","HunterOrbit66","gec1o","XxCart3rAlphaxX2005"
}

-- Create a TrussPart for each user to avoid truncation
for _, username in ipairs(users) do
    local T = Instance.new("TrussPart")
    T.Name = username
    T.Parent = P
end

print("Folder system created with", #users, "users")
